{
  "expo": {
    "name": "FHServicios",
    "slug": "FHServicios",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logoPandora_ios_1024.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elviscruz30.antapaccayapp",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "fetch",
          "remote-notification",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.elviscruz30.antapaccayapp",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoPandora_android_1024.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "name": "FHServicios",
      "favicon": "./assets/logoPandora_1024.jpg",
      "shortName": "TranscargoFH",
      "manifestPath": "./public/manifest.json",
      "icons": [
        {
          "src": "./assets/images/logo192.png",
          "sizes": "192x192",
          "type": "image/png"
        },
        {
          "src": "./assets/images/logo512.png",
          "sizes": "512x512",
          "type": "image/png"
        }
      ],
      "startUrl": "/",
      "display": "standalone",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "lang": "en",
      "orientation": "portrait"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/pandoraBanner.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-mail-composer",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a649d5d4-6ae1-43a2-9160-e0638cfa1ca5"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a649d5d4-6ae1-43a2-9160-e0638cfa1ca5"
    },
    "owner": "pandora45"
  }
}
